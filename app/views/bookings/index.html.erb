<h2>Bookings INDEX</h2>

  <div class="row">
    <div class="card-columns">
      <% @bookings.each do |booking| %>
        <div class="col-4">
          <div class="card" style="width: 18rem;">
            <div class="card-body">
                <h5 class="card-title">This Is a booking for seat <%= booking.seat_id %>.</h5>

            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
